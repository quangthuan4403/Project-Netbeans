package baitapthuan;

import java.util.Scanner;

/**
 *
 * @author pc
 */
public class BaiTapThuan {

    /**
     * @param args the command line arguments
     */
    public static void tinhSoThuc(){
        Scanner sc = new Scanner(System.in);
        double a,b;
        System.out.print("Nhap So a = ");
        a = sc.nextDouble();
        System.out.print("Nhap So b = ");
        b = sc.nextDouble();
        System.out.println("Tong la"+(a+b));
        System.out.println("Tich la "+(a*b));
        System.out.println("thuong la"+(a/b));
        System.out.println("Hieu la"+(a-b));
    }
    public static void canBacHai(){
        Scanner sc = new Scanner(System.in);
        double a;
        System.out.print("Nhap vao a");
        a = sc.nextDouble();
        System.out.printf("can bac 2 cua %.2f la %.3f",a,Math.sqrt(a));
        
    }
    public static void hinhChuNhat(){
        int dai,rong;
     Scanner sc = new Scanner(System.in);
     System.out.print("Nhap chieu dai a = ");
     dai = sc.nextInt();
     System.out.print("Nhap chieu rong b = ");
     rong = sc.nextInt();
     System.out.println("Dien tich la:"+(dai*rong));
     System.out.println("Chu vi la:"+(dai+rong)*2);

     
     
     

        
    }
    public static void soLonNhat(){
    Scanner sc = new Scanner(System.in);
    int a,b;
    System.out.print("Nhap a = ");
     a = sc.nextInt();
     System.out.print("Nhap b = ");
     b = sc.nextInt();
     System.out.println("So lon nhat la "+ Math.max(a,b));
    
        
    }
    public static void soBeNhat(){
    Scanner sc = new Scanner(System.in);
    int a,b,c;
    System.out.print("Nhap a = ");
     a = sc.nextInt();
     System.out.print("Nhap b = ");
     b = sc.nextInt();
     System.out.print("Nhap c = ");
     c = sc.nextInt();
     System.out.println("So be nhat la "+ Math.min(Math.min(a,b),c));
     
    }

    
    
    
    public static void main(String[] args) {
        // TODO code application logic here
     //   tinhSoThuc();
     //   canBacHai();
     soBeNhat();
        
        
        
        
        
    }
    
}
